{
	"branchConcurrentLimit": 1,
	"extends": [
		"config:recommended"
	],
	"ignorePaths": [
		"android"
	],
	"packageRules": [
		{
			"matchUpdateTypes": [
				"minor",
				"patch",
				"pin",
				"digest"
			],
			"automerge": true,
			"automergeType": "branch"
		},
		{
			"matchDepTypes": [
				"devDependencies"
			],
			"automerge": true,
			"automergeType": "branch"
		}
	],
	"postUpdateOptions": [
		"yarnDedupeHighest"
	]
}
